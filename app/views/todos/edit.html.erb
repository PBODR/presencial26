<h1>EDIT</h1>

<div class="well">
  <%= form_with model: @todo, local: true do |form| %>
   <%= form.label :description %>
   <%= form.text_area :description, id: :description %>

   <%= form.label :completed, "Completado" %>
   <%= form.check_box :completed, id: :completed %>

   <%= form.submit "Crear" %>
  <% end %>
</div>
