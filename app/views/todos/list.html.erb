<div class="col-md-6">
    <h1>TRUE</h1>
  <% @todos.each do |t| %>
    <% if t.completed == true %>
      <div class="well">
        <p><%= t.description %></p>
        <p><%= t.completed %></p>
      </div>
    <% end %>
  <% end %>
</div>

<div class="col-md-6">
  <h1>FALSE</h1>
  <% @todos.each do |t| %>
    <% if t.completed == false %>
      <div class="well">
        <p><%= t.description %></p>
        <p><%= t.completed %></p>
      </div>
    <% end %>
  <% end %>
</div>
